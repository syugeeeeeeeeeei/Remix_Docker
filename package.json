{
  "name": "remix-docker",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up --build",
    "build": "docker-compose build",
    "stop": "docker-compose down",
    "typecheck": "yarn workspaces foreach -pt run typecheck"
  },
  "packageManager": "yarn@4.9.2",
  "dependencies": {
    "typescript": "^5.8.3"
  }
}
