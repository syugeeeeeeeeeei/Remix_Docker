{
  "name": "remix-docker",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "yarn workspaces foreach --all  -t run build",
    "dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up --build",
    "stop": "docker-compose down",
    "typecheck": "yarn workspaces foreach -pt run typecheck"
  },
  "packageManager": "yarn@4.9.2"
}
